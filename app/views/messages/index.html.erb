<h1>Inbox</h1>
<% @messages.each do |message| %>
  <div id="message-li">
    <li >
      <p>
        Subject:
        <%= message.subject %>
      </p>

      <p>
        Message:
        <%= message.body %>
      </p>

      <p>
        Sent by:
        <%= message.updated_at %>
      </p>

      <p>
        Sent from:
        <%= message.sender_id %>
      </p>
    </li>
  </div>
<% end %>
